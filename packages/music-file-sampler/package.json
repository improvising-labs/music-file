{
  "name": "music-file-sampler",
  "version": "0.1.5",
  "type": "module",
  "module": "./dist/esm/index.js",
  "main": "./dist/commonjs/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/commonjs/index.js"
    }
  },
  "license": "MIT",
  "author": "Improvising",
  "description": "Manage and play MusicFile samples using on Web Audio API",
  "devDependencies": {
    "@types/node": "^18.0.6",
    "fix-esm-import-paths": "^0.1.3",
    "music-file": "^0.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {},
  "peerDependencies": {
    "music-file": "^0.1.0"
  },
  "scripts": {
    "build:commonjs": "tsc --project tsconfig.commonjs.json",
    "build:esm": "tsc --project tsconfig.esm.json && fix-esm-import-paths ./dist/esm/",
    "build": "rimraf dist && yarn build:commonjs && yarn build:esm"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
